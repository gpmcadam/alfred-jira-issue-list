{
  "name": "alfred-jira-issue-list",
  "type": "module",
  "version": "1.3.2",
  "description": "JIRA",
  "main": "index.js",
  "scripts": {
    "postinstall": "alfy-init",
    "preuninstall": "alfy-cleanup",
    "lint": "eslint .",
    "prettier": "prettier --check \"**/*.{js,json}\"",
    "prettify": "prettier --write \"**/*.{js,json}\"",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage"
  },
  "jest": {
    "transform": {},
    "moduleFileExtensions": [
      "mjs",
      "js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "author": {
    "name": "Gary McAdam",
    "email": "gary@mcad.am",
    "url": "https://gary.mcad.am"
  },
  "license": "ISC",
  "dependencies": {
    "alfy": "^3.0.0",
    "got": "^12.1.0",
    "plist": "^3.1.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^28.0.0",
    "jest": "^29.0.0",
    "nock": "^13.5.0",
    "prettier": "^3.0.0"
  },
  "overrides": {
    "plist": "^3.1.0",
    "xmldom": "^0.8.10",
    "cross-spawn": "^7.0.6",
    "package-json": "^8.1.1",
    "latest-version": "^7.0.0"
  }
}
