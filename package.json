    "url": "https://gary.mcad.am"
  },
  "license": "ISC",
  "dependencies": {
    "alfy": "^0.9.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.5"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.7.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1"
  }
}
